ontent\">\n                        <img :src=\"getImg(project.imgsrc)\" />\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"material-symbols-outlined\">\n            chevron_right\n        </div>\n    </div>\n\n</template>\n\n<script>\nexport default {\n    inject: ['projects'],\n    data() {\n        return {\n            index: 0,\n            project: this.projects[this.index],\n        }\n    },\n    methods: {\n        openLink(link) {\n            window.open(link, '_blank')\n        },\n        getImg(imgsrc) {\n            return require('../assets/' + imgsrc)\n        },\n        nextPage() {\n            this.index\n        }\n        \n    },\n    computed: {\n       \n    },\n    mounted(){\n        console.log(this.project)\n    }\n\n}\n</script>\n\n<style lang=\"scss\">\n\n$stroke: #3c4043;\n$shadow: #6B9080;\n$body: #F6FFF8;\n$med: #A4C3B2;\n$light: #EAF4F4;\n$r: 'Roboto';\n$o: 'Alfa Slab One';\n$c: 'Comic Neue';\n.material-symbols-outlined {\n    font-size: 50px;\n    color: $shadow;\n    cursor: pointer;\n    font-variation-settings:\n        'FILL' 0,\n        'wght' 400,\n        'GRAD' 0,\n        'opsz' 48,\n        \n}\n\n.projects-list {\n    max-width: 800px;\n    margin: 0 auto;\n    display:flex;\n    align-items: center;\n\n    .project-item {\n        display: flex;\n        flex-basis: auto;\n        flex-wrap: nowrap;\n        align-items: stretch;\n        font-family: $r, sans-serif;\n        color: $stroke;\n\n        div {\n            text-align: left;\n            padding-left: 10px;\n\n        }\n\n        h5 {\n            font-size: 1.5rem;\n            font-family: $r, sans-serif;\n            letter-spacing: 0.3rem;\n            color: $med;\n            text-decoration: underline;\n        }\n\n        ul {\n            display: inline-flex;\n            list-style: none;\n            font-family: $r, sans-serif;\n            margin-top: -20px;\n            margin-left: -20px;\n            \n\n            li {\n                padding: 7px 15px;\n                background-color: $shadow;\n                margin-right: 10px;\n                color: $light;\n            }\n        }\n\n        .git-link {\n            background-color: #FFFFFF;\n            border: 1px solid $shadow;\n            border-radius: 8px;\n            box-sizing: border-box;\n            color: $shadow;\n            cursor: pointer;\n            display: inline-block;\n            font-family: Circular, -apple-system, BlinkMacSystemFont, 'Roboto', \"Helvetica Neue\", sans-serif;\n            font-size: 16px;\n            font-weight: 400;\n            line-height: 20px;\n            margin: 20px 0px 0px 60px;\n            outline: none;\n            padding: 7px 15px;\n            position: relative;\n            text-align: center;\n            text-decoration: none;\n            touch-action: manipulation;\n            transition: box-shadow .2s, -ms-transform .1s, -webkit-transform .1s, transform .1s;\n            user-select: none;\n            -webkit-user-select: none;\n            width: auto;\n\n\n            &:focus-visible {\n                box-shadow: $shadow 0 0 0 2px, rgba(255, 255, 255, 0.8) 0 0 0 4px;\n                transition: box-shadow .2s;\n            }\n\n            &:active {\n                background-color: #F7F7F7;\n                border-color: $shadow;\n                transform: scale(.96);\n            }\n\n            &:disabled {\n                border-color: #DDDDDD;\n                color: #DDDDDD;\n                cursor: not-allowed;\n                opacity: 1;\n            }\n        }\n\n        .img-dot {\n            height: 12px;\n            width: 12px;\n            background-color: $med;\n            border-radius: 50%;\n            display: inline-block;\n        }\n\n        .img-container {\n            max-width: 390px;\n            border: 3px solid $light;\n            border-top-left-radius: 4px;\n            border-top-right-radius: 4px;\n        }\n\n        .top {\n            padding: 10px;\n            margin-left: -10px;\n            background: $light;\n            border-top-left-radius: 4px;\n            border-top-right-radius: 4px;\n\n            .open-btn {\n                margin-left: 18rem;\n                padding: 2px 10px 4px 10px;\n                color: #fff;\n                background-color: $med;\n                border-radius: 4px;\n                font-family: monospace;\n                border: 0.1rem solid $med;\n                cursor: pointer;\n\n                &:active {\n                    background-color: $light;\n                    color: $shadow;\n                }\n            }\n        }\n\n\n        .img-content {\n            margin-left: -20px;\n            margin-bottom: -10px;\n            img {\n                max-width: 400px;\n            }\n        }\n\n    }\n}\n</style>"]},"metadata":{},"sourceType":"module"}