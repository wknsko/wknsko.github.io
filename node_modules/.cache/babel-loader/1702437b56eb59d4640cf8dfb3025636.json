inus') {\n                this.index = this.projects.length -1\n            } else if (symbol === 'plus' && this.index === this.projects.length - 1) {\n                this.index = 0;\n            } else if (symbol === 'minus' ) {\n                this.index--\n            } else if (symbol === 'plus' ) {\n                this.index++\n            }\n            \n            \n        }\n        \n    },\n    computed: {\n       \n    },\n    mounted(){\n        if (window.innerWidth <= 550) {\n            this.grid = 'project-grid'\n        }\n    }\n\n}\n</script>\n\n<style lang=\"scss\">\n\n$stroke: #3c4043;\n$shadow: #6B9080;\n$body: #F6FFF8;\n$med: #A4C3B2;\n$light: #EAF4F4;\n$r: 'Roboto';\n$o: 'Alfa Slab One';\n$c: 'Comic Neue';\n\n.info {\n    height:300px\n}\n\nimg-container {\n    margin-left:20px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}