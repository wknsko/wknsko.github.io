: 1px solid $shadow;\n  box-shadow: 6px 6px $shadow;\n  padding: 30px;\n  background-color:#fff;\n  .icons {\n    display:flex;\n    margin-right: auto;\n    ul {\n      margin-left: auto;\n      display: flex;\n      list-style: none;\n      li {\n        padding-left: 10px;\n        cursor: pointer;\n      }\n    }\n  }\n  .message-body {\n    text-align: justify;\n  .message-name {\n    font-family: $r, sans-serif;\n    font-size: 2.8rem;\n    color: $stroke;\n  }\n  .message-desc {\n    font-family: $r, serif;\n    font-weight: 300;\n    font-size: 1.5rem;\n    color: $med;\n    overflow: hidden;\n    border-right: .15em solid $med;\n    white-space: nowrap;\n    margin: 0 auto;\n    letter-spacing: .15em;\n    animation:\n    typing 6.5s steps(40, end),\n    blink-caret .75s step-end infinite;\n  }\n    @keyframes typing {\n      from {\n        width: 0\n      }\n  \n      to {\n        width: 100%\n      }\n    }\n  \n    @keyframes blink-caret {\n  \n      from,\n      to {\n        border-color: transparent\n      }\n  \n      50% {\n        border-color: $med\n      }\n    }\n  \n  \n  }\n  .contact-text {\n    padding: 60px 0px 30px 0;\n    text-align: center;\n    font-family: 'Courier New', Courier, monospace;\n    font-weight: 600;\n    font-size: 1.1rem;\n    color: $stroke;\n    cursor: pointer;\n  }\n    .contact-body {\n      text-align: center;\n      font-family: monospace;\n      color: $shadow;\n      font-size: 0.8rem;\n      letter-spacing: 0.1rem;\n      ul {\n        list-style: none;\n      }\n    }\n}\n\n\n\n\n\n</style>\n"]},"metadata":{},"sourceType":"module"}